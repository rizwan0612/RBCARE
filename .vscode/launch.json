    "version": "0.2.0",
    "configurations": [
        {
            "type": "node",
            "request": "launch",
            "name": "Run swagger-jsdoc",
            "runtimeExecutable": "swagger-jsdoc",
            "cwd": "${workspaceFolder}",
            "args": []
        },
        {
            "type": "chrome",
            "request": "launch",
            "name": "Launch Chrome against localhost",
            "url": "http://localhost:3002/api",
            "webRoot": "${workspaceFolder}/src/server.ts",
            "env": {
                "NODE_ENV": "development",
            }
        }
    ]
}